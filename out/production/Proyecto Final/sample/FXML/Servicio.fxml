<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.shape.Box?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import de.jensd.fx.glyphs.materialicons.MaterialIconView?>
<?import javafx.scene.control.TableColumn?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
    <ComboBox fx:id="cmbTipoServicios"    style="-fx-font-size: 20" prefWidth="500" prefHeight="15" AnchorPane.leftAnchor="80" AnchorPane.topAnchor="10"/>
    <TableView fx:id="tabla" style="-fx-font-size: 20" prefWidth="500" prefHeight="550" AnchorPane.leftAnchor="80" AnchorPane.topAnchor="60">
        <columns>
            <TableColumn editable="false" fx:id="clmCantidad" style="-fx-font-size: 20" prefWidth="475"/>
        </columns>
        <columns>
            <TableColumn editable="false" visible="false" fx:id="clmOtros" style="-fx-font-size: 20" prefWidth="237.5"/>
        </columns>
    </TableView>
    <TextField fx:id="textComision" text="Comision"                   style="-fx-font-size: 20" prefWidth="500" AnchorPane.rightAnchor="40" AnchorPane.topAnchor="60"/>
    <TextField fx:id="textPago" text="Pago"                           style="-fx-font-size: 20" prefWidth="500" AnchorPane.rightAnchor="40" AnchorPane.topAnchor="140"/>
    <TextField fx:id="textNumeReferencia" text="Numero de Referencia" style="-fx-font-size: 20" prefWidth="500" AnchorPane.rightAnchor="40" AnchorPane.topAnchor="190"/>
    <TextField fx:id="textTelefono" text="Telefono"                   style="-fx-font-size: 20" prefWidth="500" AnchorPane.rightAnchor="40" AnchorPane.topAnchor="140"/>
    <TextField fx:id="textConfTelefono" text="Confirmar Telefono"     style="-fx-font-size: 20" prefWidth="500" AnchorPane.rightAnchor="40" AnchorPane.topAnchor="190"/>

    <Button fx:id="btnRegresar" prefHeight="30" AnchorPane.topAnchor="0" AnchorPane.leftAnchor="0">         <!--- Boton Pago de Servicio domestico -->
        <graphic>
            <MaterialIconView glyphName="KEYBOARD_ARROW_LEFT" glyphSize="15" fill="azure"/>
        </graphic>
    </Button>

    <Button fx:id="btnAceptar" text="Aceptar" style="-fx-font-size: 20" prefWidth="500" AnchorPane.rightAnchor="40" AnchorPane.topAnchor="240"/>
</AnchorPane>
