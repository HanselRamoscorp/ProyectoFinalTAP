<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import de.jensd.fx.glyphs.materialicons.MaterialIconView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
    <Button fx:id="btnRegresar" prefHeight="30" AnchorPane.topAnchor="0" AnchorPane.leftAnchor="0">         <!--- Boton Pago de Servicio domestico -->
        <graphic>
            <MaterialIconView glyphName="KEYBOARD_ARROW_LEFT" glyphSize="15" fill="azure"/>
        </graphic>
    </Button>
    <TableView fx:id="tablaHomeService" style="-fx-font-size: 20" prefWidth="500" prefHeight="550" AnchorPane.rightAnchor="80" AnchorPane.topAnchor="70">
        <columns>
            <TableColumn fx:id="clmNombreHomeService"  text="Empresa" editable="false" style="-fx-font-size: 20" prefWidth="250"/>
            <TableColumn fx:id="clmHomeService"        text="Cantidad" editable="false" style="-fx-font-size: 20" prefWidth="250"/>
        </columns>
    </TableView>
    <TableView fx:id="tablaRecharge" style="-fx-font-size: 20" prefWidth="500" prefHeight="550" AnchorPane.leftAnchor="80" AnchorPane.topAnchor="70">
        <columns>
            <TableColumn fx:id="clmNombreRecharge"  text="Compañia" editable="false" style="-fx-font-size: 20" prefWidth="250"/>
            <TableColumn fx:id="clmRecharge"        text="Recargas" editable="false" style="-fx-font-size: 20" prefWidth="250"/>
        </columns>
    </TableView>
    <TextField fx:id="textTotalRecharge" text="Nombre Compañia" style="-fx-font-size: 20" prefWidth="500" AnchorPane.leftAnchor="80" AnchorPane.topAnchor="630"/>
    <TextField fx:id="textTotalHomeService" text="Nombre Compañia" style="-fx-font-size: 20" prefWidth="500" AnchorPane.rightAnchor="80" AnchorPane.topAnchor="630"/>
</AnchorPane>
