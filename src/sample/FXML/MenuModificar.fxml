<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import de.jensd.fx.glyphs.materialicons.MaterialIconView?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import de.jensd.fx.glyphs.materialdesignicons.MaterialDesignIconView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
    <Button fx:id="btnRegresar" prefHeight="30" AnchorPane.topAnchor="0" AnchorPane.leftAnchor="0">         <!--- Boton Pago de Servicio domestico -->
        <graphic>
            <MaterialIconView glyphName="KEYBOARD_ARROW_LEFT" glyphSize="15" fill="azure"/>
        </graphic>
    </Button>
    <Button fx:id="btnSeleccionar" text="Servicios" contentDisplay="TOP"  prefWidth="200" AnchorPane.topAnchor="150" AnchorPane.leftAnchor="300">         <!--- Boton Modificar servicios -->
        <graphic>
            <FontAwesomeIconView glyphName="CLIPBOARD" glyphSize="200" fill= "azure"/>
        </graphic>
    </Button>
    <Button fx:id="btnActualizar" text="Modificar servicios" contentDisplay="TOP"  prefWidth="200" AnchorPane.topAnchor="150" AnchorPane.leftAnchor="520">         <!--- Boton Modificar servicios -->
        <graphic>
            <FontAwesomeIconView glyphName="EDIT" glyphSize="200" fill= "azure"/>
        </graphic>
    </Button>
    <Button fx:id="btnEliminar" text="Eliminar servicios" contentDisplay="TOP" prefWidth="200" AnchorPane.topAnchor="150" AnchorPane.leftAnchor="740">         <!--- Boton Modificar servicios -->
        <graphic>
            <MaterialDesignIconView glyphName="DELETE" glyphSize="200" fill= "azure"/>
        </graphic>
    </Button>
    <Button fx:id="btnAgregar" text="Agregar servicios" contentDisplay="TOP" prefWidth="200" AnchorPane.topAnchor="150" AnchorPane.leftAnchor="960">         <!--- Boton Modificar servicios -->
        <graphic>
            <FontAwesomeIconView glyphName="EDIT" glyphSize="200" fill= "azure"/>
        </graphic>
    </Button>
    <Button fx:id="btnReportes" text="Reporte de ventas" contentDisplay="TOP" prefWidth="200" AnchorPane.topAnchor="150" AnchorPane.leftAnchor="1180">         <!--- Boton Modificar servicios -->
        <graphic>
            <MaterialIconView glyphName="ARCHIVE" glyphSize="200" fill= "azure"/>
        </graphic>
    </Button>
</AnchorPane>
